{
	"name": "PageSummaries",
	"version": "1.2",
	"author": [
		"Colin Andrew Ferm"
	],
	"url": "https://github.com/colinferm/wiki-page-summaries",
	"descriptionmsg": "pagesummaries-desc",
	"license-name": "BSD-3-Clause",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.32.0"
	},
	"MessagesDirs": {
